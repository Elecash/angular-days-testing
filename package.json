{
  "name": "angular-days-testing",
  "version": "1.0.0",
  "description": "AngularJS Days Testing Workshop",
  "main": "index.js",
  "scripts": {
    "start": "http-server -c-1 -o -s -p 9875 ./",
    "coverage": "http-server -c-1 -o -s -p 9876 ./coverage",
    "test": "karma start tests/conf/karma.conf.js",
    "e2e": "parallelshell 'npm run e2e-server' 'npm run watch:e2e'",
    "e2e-tests": "protractor tests/conf/e2e.conf.js",
    "e2e-server": "http-server -c-1 -s -p 9877 ./",
    "watch:e2e": "watch 'npm run e2e-tests' tests/e2e"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Elecash/angular-days-testing.git"
  },
  "author": "Raul Jimenez <elecash@gmail.com> (http://twofuckingedevelopers.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Elecash/angular-days-testing/issues"
  },
  "homepage": "https://github.com/Elecash/angular-days-testing#readme",
  "dependencies": {
    "angular": "1.5.0",
    "angular-animate": "1.5.0",
    "angular-resource": "1.5.0",
    "angular-route": "1.5.0",
    "angular-strap": "2.3.7",
    "bootstrap": "3.3.6"
  },
  "devDependencies": {
    "angular-mocks": "1.5.0",
    "http-server": "0.9.0",
    "jasmine-core": "2.4.1",
    "karma": "0.13.21",
    "karma-coverage": "0.5.4",
    "karma-jasmine": "0.3.7",
    "karma-ng-html2js-preprocessor": "0.2.1",
    "karma-phantomjs-launcher": "1.0.0",
    "karma-spec-reporter": "0.0.24",
    "parallelshell": "2.0.0",
    "phantomjs-prebuilt": "2.1.4",
    "watch": "0.17.1"
  }
}
